<?xml version="1.0" encoding="utf-8"?>
<android.support.v4.widget.DrawerLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/naranjapatadas"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.example.jessi.tae.naranjapatadasActivity">

    <RelativeLayout
        android:background="@drawable/fondo2"
        android:layout_width="match_parent"
        android:layout_height="match_parent">

        <TextView
            android:id="@+id/textViewTitulo"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentLeft="true"
            android:layout_alignParentStart="true"
            android:layout_below="@drawable/fondo2"
            android:text="naranja patadas"
            android:textColor="@color/colorsubtitulo"
            android:textSize="@dimen/tamanosubtitulo"
            tools:layout_editor_absoluteX="138dp"
            tools:layout_editor_absoluteY="191dp" />

        <ImageButton
            android:id="@+id/botonvolver1"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentLeft="true"
            android:layout_alignParentStart="true"
            android:layout_below="@+id/textViewTitulo"
            android:layout_marginTop="25dp"
            android:onClick="Volver"
            app:srcCompat="@drawable/apsogui_m"
            android:text="Volver a naranja" />

        <ImageButton
            android:id="@+id/button3"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentLeft="true"
            android:layout_alignParentStart="true"
            android:layout_below="@+id/textViewTitulo"
            android:layout_marginTop="100dp"
            android:onClick="Volver"
            app:srcCompat="@drawable/apsogui_m"
            android:text="Volver a naranja" />

    </RelativeLayout>

    <android.support.design.widget.NavigationView
        android:id="@+id/navnaranja"
        app:headerLayout="@layout/header"
        android:layout_height="match_parent"
        android:layout_width="wrap_content"
        android:background="@color/colorAccent"
        app:itemTextColor="@color/colorPrimary"
        app:menu="@menu/drawermenu"
        android:layout_gravity="start">
    </android.support.design.widget.NavigationView>


</android.support.v4.widget.DrawerLayout>
